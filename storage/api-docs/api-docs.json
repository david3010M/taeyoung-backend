            "post": {
                "tags": [
                    "Supplier"
                ],
                "summary": "Create a new supplier",
                "operationId": "b52ec72b1bfd83099673918c9abddb0e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "example": "JURIDICA"
                                    },
                                    "ruc": {
                                        "type": "string",
                                        "example": "20547869541"
                                    },
                                    "businessName": {
                                        "type": "string",
                                        "example": "Distribuidora de Productos S.A."
                                    },
                                    "address": {
                                        "type": "string",
                                        "example": "Jr. Los Pinos 123"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "supplier@gmail.com"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "example": "987654321"
                                    },
                                    "representativeDni": {
                                        "type": "string",
                                        "example": "12345678"
                                    },
                                    "representativeNames": {
                                        "type": "string",
                                        "example": "Juan Perez"
                                    },
                                    "country_id": {
                                        "type": "integer",
                                        "example": "1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Supplier created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Supplier"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
